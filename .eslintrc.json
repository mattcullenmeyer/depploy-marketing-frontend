{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint", "react"],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier",
    "next"
  ],
  "rules": {
    "import/prefer-default-export": "off",
    "import/no-default-export": "off",
    "react/react-in-jsx-scope": "off"
  }
}
